<!-- jQuery, Bootstrap JS, DataTables JS, and SweetAlert2 -->
<script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
<script src="https://cdn.datatables.net/1.10.24/js/jquery.dataTables.min.js"></script>
<script src="https://cdn.datatables.net/1.10.24/js/dataTables.bootstrap4.min.js"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

<script>
  $(document).ready(function() {
    console.log('Document ready');

    // ── Helpers ──────────────────────────────────────────────────────
    function normalizeDT(s) {
      return s ? s.replace('T', ' ').substr(0, 16) : '';
    }

    function showFieldError($input, msg) {
      $input.addClass('is-invalid');
      if (!$input.next('.invalid-feedback').length) {
        $input.after('<div class="invalid-feedback">' + msg + '</div>');
      }
    }

    function clearFieldError($input) {
      $input.removeClass('is-invalid');
      $input.next('.invalid-feedback').remove();
    }
    // ─────────────────────────────────────────────────────────────────

    var currentObjectId;
    var callFollowUpTable;
    var arrivalFollowUpTable;
    var departureFollowUpTable;

    $('#markupTable').DataTable();

    // ── Open Follow-up Modal ─────────────────────────────────────────
    $(document).on('click', '.open-modal', function() {
      currentObjectId = $(this).data('object_id');
      console.log('Open-followup modal for object', currentObjectId);
      $('#followupModalTitle').text($(this).data('enquiry_name'));
      $('#object_id_form').val(currentObjectId);
      $.ajax({
        url: "<?= site_url('Enwuiryfollowup/getFollowupTypes'); ?>",
        type: "POST",
        data: {
          enterprise_id: $(this).data('enterprise')
        },
        dataType: "json",
        success: function() {
          $('#followupModal').modal('show');
        }
      });
    });

    // ── Call Follow-ups (common modal) ────────────────────────────────
    $(document).on('click', '.common-modal-btn', function() {
      var followupTypeId = $(this).data('followup-id'),
        followupType = $(this).data('followup');
      $('#commonModalLabel').text(followupType + ' Follow-ups');
      $('#followup_type_id_form').val(followupTypeId);
      $('#commonModal').modal('show');

      if ($.fn.DataTable.isDataTable('#callFollowUpTable'))
        $('#callFollowUpTable').DataTable().destroy();

      callFollowUpTable = $('#callFollowUpTable').DataTable({
        ajax: {
          url: "<?= site_url('Enwuiryfollowup/getCallFollowups'); ?>",
          type: "POST",
          data: {
            followupTypeId: followupTypeId,
            objectId: currentObjectId
          },
          dataSrc: 'data'
        },
        columns: [{
            data: 'call_time',
            title: 'Call Time'
          },
          {
            data: 'call_status',
            title: 'Status'
          },
          {
            data: 'comments',
            title: 'Comments'
          },
          {
            data: null,
            title: 'Action',
            orderable: false,
            render: row => `
            <i class="fas fa-edit text-success edit-btn" data-id="${row.call_follow_up_id}" style="cursor:pointer"></i>
            <i class="fas fa-trash-alt text-success delete-btn ml-2" data-id="${row.call_follow_up_id}" style="cursor:pointer"></i>
          `
          }
        ]
      });
    });

    // ── Add Call Follow-up ────────────────────────────────────────────
    $('#btnAddFollowUp').on('click', function() {
      $('#addFollowUpModalLabel').text('Add New Follow-up');
      $('#addFollowupForm')[0].reset();
      $('#call_follow_up_id_form').remove();
      $('#addFollowUpModal').modal('show');
    });

    // ── Edit Call Follow-up ───────────────────────────────────────────
    $('#callFollowUpTable tbody').on('click', '.edit-btn', function() {
      var id = $(this).data('id');
      $.ajax({
        url: "<?= site_url('Enwuiryfollowup/getCallFollowupById'); ?>",
        type: "POST",
        data: {
          id: id
        },
        dataType: "json",
        success: function(data) {
          if (!data.status) {
            $('#call_time_form').val(data.call_time);
            $('#call_status_form').val(data.call_status);
            $('#comments_form').val(data.comments);
            $('#call_follow_up_id_form').remove();
            $('<input>').attr({
                type: 'hidden',
                id: 'call_follow_up_id_form',
                name: 'call_follow_up_id',
                value: data.call_follow_up_id
              })
              .appendTo('#addFollowupForm');
            $('#addFollowUpModalLabel').text('Edit Follow-up');
            $('#addFollowUpModal').modal('show');
          }
        }
      });
    });

    // ── Delete Call Follow-up ─────────────────────────────────────────
    $('#callFollowUpTable tbody').on('click', '.delete-btn', function() {
      var id = $(this).data('id');
      Swal.fire({
        title: 'Are you sure?',
        text: "This cannot be undone.",
        icon: 'warning',
        showCancelButton: true,
        confirmButtonColor: '#28a745',
        cancelButtonColor: '#3085d6',
        confirmButtonText: 'Yes, delete it!'
      }).then(result => {
        if (result.isConfirmed) {
          $.post("<?= site_url('Enwuiryfollowup/deleteCallFollowup'); ?>", {
            id: id
          }, function(res) {
            if (res.status === 'success') {
              Swal.fire('Deleted!', 'Follow-up removed.', 'success');
              callFollowUpTable.ajax.reload();
            } else {
              Swal.fire('Error', res.message, 'error');
            }
          }, 'json');
        }
      });
    });

    // ── Save Call Follow-up ───────────────────────────────────────────
    $('#addFollowupForm').on('submit', function(e) {
      e.preventDefault();
      var isEdit = $('#call_follow_up_id_form').length > 0,
        $time = $('#call_time_form');
      clearFieldError($time);

      if (!isEdit) {
        var newDT = normalizeDT($time.val()),
          existing = callFollowUpTable.column(0).data().toArray().map(normalizeDT);
        if (existing.includes(newDT)) {
          showFieldError($time, 'A call follow-up at that time already exists.');
          return;
        }
      }

      var url = isEdit ?
        "<?= site_url('Enwuiryfollowup/updateCallFollowup'); ?>" :
        "<?= site_url('Enwuiryfollowup/saveCallFollowup'); ?>";

      $.post(url, $(this).serialize(), function(res) {
        if (res.status === 'success') {
          Swal.fire('Saved!', '', 'success');
          $('#addFollowUpModal').modal('hide');
          callFollowUpTable.ajax.reload();
        } else {
          Swal.fire('Error', res.message, 'error');
        }
      }, 'json');
    });

    // ── ARRIVAL Follow-ups ────────────────────────────────────────────
    $('#arrivalDetailsModal').on('shown.bs.modal', function() {
      var typeId = $(this).data('followup-id') || 2;
      if ($.fn.DataTable.isDataTable('#arrivalFollowUpTable'))
        $('#arrivalFollowUpTable').DataTable().destroy();

      arrivalFollowUpTable = $('#arrivalFollowUpTable').DataTable({
        ajax: {
          url: "<?= site_url('Enwuiryfollowup/getArrivalFollowups'); ?>",
          type: "POST",
          data: {
            followupTypeId: typeId,
            objectId: currentObjectId
          },
          dataSrc: 'data'
        },
        columns: [{
            data: 'call_date',
            title: 'Call Date',
            render: d => d ? d.split(' ')[0] : ''
          },
          {
            data: 'call_date',
            title: 'Call Time',
            render: d => d ? d.split(' ')[1] || '' : ''
          },
          {
            data: 'mode_of_arrival',
            title: 'Mode'
          },
          {
            data: 'city',
            title: 'City'
          },
          {
            data: 'flight_train_no',
            title: 'Flight/Train Number'
          },
          {
            data: 'arrival_date',
            title: 'Arrival Date',
            render: d => d ? d.split(' ')[0] : ''
          },
          {
            data: 'arrival_date',
            title: 'Time',
            render: d => d ? d.split(' ')[1] || '' : ''
          },
          {
            data: 'comments',
            title: 'Comments'
          },
          {
            data: null,
            title: 'Action',
            orderable: false,
            render: r => `
            <i class="fas fa-edit text-success editArrival" data-id="${r.arrival_follow_up_id}" style="cursor:pointer"></i>
            <i class="fas fa-trash-alt text-success deleteArrival ml-2" data-id="${r.arrival_follow_up_id}" style="cursor:pointer"></i>
          `
          }
        ]
      });
    });

    $('#btnArrivalFollowUp').on('click', function() {
      $('#arrivalFollowupForm')[0].reset();
      $('#arrival_follow_up_id').val('');
      $('#addarrivalFollowUpModalLabel').text('Add New Arrival Followup');
      $('#arrival_followup_type_id_form').val(2);
      $('#arrival_object_id_form').val(currentObjectId);
      $('#arrivalFollowUpModal').modal('show');
    });

    $('#arrivalFollowupForm').on('submit', function(e) {
      e.preventDefault();
      var isEdit = $('#arrival_follow_up_id').val() !== '',
        $cd = $('#call_date_form'),
        $ct = $('#arrival_call_time_form'),
        $ad = $('#arrival_form'),
        $at = $('#time_form');
      [$cd, $ct, $ad, $at].forEach(clearFieldError);

      if (!isEdit) {
        var callDT = normalizeDT($cd.val() + 'T' + $ct.val()),
          arrDT = normalizeDT($ad.val() + 'T' + $at.val()),
          dup = arrivalFollowUpTable.rows().data().toArray().some(r =>
            normalizeDT(r.call_date) === callDT && normalizeDT(r.arrival_date) === arrDT
          );
        if (dup) {
          showFieldError($ct, 'An arrival at that date/time already exists.');
          showFieldError($at, 'An arrival at that date/time already exists.');
          return;
        }
      }

      $.post('<?= site_url("Enwuiryfollowup/saveArrivalFollowup"); ?>', $(this).serialize(), function(res) {
        if (res.status === 'success') {
          Swal.fire('Saved!', 'Your arrival follow-up has been saved.', 'success');
          $('#arrivalFollowUpModal').modal('hide');
          arrivalFollowUpTable.ajax.reload();
        } else {
          Swal.fire('Error!', res.message, 'error');
        }
      }, 'json');
    });

    $(document).on('click', '.editArrival', function() {
      // TODO: Implement editArrival
    });

    $(document).on('click', '.deleteArrival', function() {
      // TODO: Implement deleteArrival
    });

    // ── DEPARTURE Follow-ups ─────────────────────────────────────────
    $('#departureDetailsModal').on('shown.bs.modal', function() {
      var typeId = $(this).data('followup-id') || 3;
      if ($.fn.DataTable.isDataTable('#departureFollowUpTable'))
        $('#departureFollowUpTable').DataTable().destroy();

      departureFollowUpTable = $('#departureFollowUpTable').DataTable({
        ajax: {
          url: "<?= site_url('Enwuiryfollowup/getDepartureFollowups'); ?>",
          type: "POST",
          data: {
            followupTypeId: typeId,
            objectId: currentObjectId
          },
          dataSrc: 'data'
        },
        columns: [{
            data: 'call_date',
            title: 'Call Date',
            render: d => d ? d.split(' ')[0] : ''
          },
          {
            data: 'call_date',
            title: 'Call Time',
            render: d => d ? d.split(' ')[1] || '' : ''
          },
          {
            data: 'mode_of_departure',
            title: 'Mode'
          },
          {
            data: 'city',
            title: 'City'
          },
          {
            data: 'flight_train_no',
            title: 'Flight/Train Number'
          },
          {
            data: 'departure_date',
            title: 'Departure Date',
            render: d => d ? d.split(' ')[0] : ''
          },
          {
            data: 'departure_date',
            title: 'Time',
            render: d => d ? d.split(' ')[1] || '' : ''
          },
          {
            data: 'comments',
            title: 'Comments'
          },
          {
            data: null,
            title: 'Action',
            orderable: false,
            render: r => `
            <i class="fas fa-edit text-success editDeparture" data-id="${r.departure_follow_up_id}" style="cursor:pointer"></i>
            <i class="fas fa-trash-alt text-success deleteDeparture ml-2" data-id="${r.departure_follow_up_id}" style="cursor:pointer"></i>
          `
          }
        ]
      });
    });

    // whenever the Add Call-FollowUp modal is about to show…
    $('#addFollowUpModal').on('show.bs.modal', function() {
      // clear every field error in that form
      $('#addFollowupForm .is-invalid').removeClass('is-invalid');
      $('#addFollowupForm .invalid-feedback').remove();
    });

    // same for Arrival modal
    $('#arrivalFollowUpModal').on('show.bs.modal', function() {
      $('#arrivalFollowupForm .is-invalid').removeClass('is-invalid');
      $('#arrivalFollowupForm .invalid-feedback').remove();
    });

    // same for Departure modal
    $('#departureFollowUpModal').on('show.bs.modal', function() {
      $('#departureFollowupForm .is-invalid').removeClass('is-invalid');
      $('#departureFollowupForm .invalid-feedback').remove();
    });


  });
</script>


<link href="https://cdn.quilljs.com/1.3.6/quill.snow.css" rel="stylesheet">
<!-- <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script> -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/html2canvas/1.4.1/html2canvas.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/2.5.1/jspdf.umd.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/quill@1.3.7/dist/quill.min.js"></script>

<script>
  let quill;
  let currentGuestName = '';

  // Build/update the placard content in Quill
  function updatePlacard() {
    if (!quill) return;
    quill.setContents([
      { insert: 'PSV Tours & Travels\n', attributes: { header: 1, align: 'center' } },
      { insert: 'Welcomes\n',            attributes: { header: 2, align: 'center' } },
      { insert: currentGuestName + '\n', attributes: { header: 2, align: 'center' } }
    ]);
    document.getElementById('followupModalTitle').innerText = currentGuestName;
  }

  // When the modal is shown, grab the guest name from the triggering element
  $('#placardModal').on('show.bs.modal', function (e) {
  // e.relatedTarget is the element (your "Placard" button) that opened the placardModal
  const triggerBtn = $(e.relatedTarget);

  // Find the closest ancestor modal, then grab its .modal-header .modal-title text
  const sourceModal = triggerBtn.closest('.modal');
  const guestName = sourceModal.find('.modal-header .modal-title').first().text().trim() || 'Guest';

  currentGuestName = guestName;

  if (!quill) {
    quill = new Quill('#editor', {
      modules: { toolbar: '#toolbar' },
      theme: 'snow'
    });
  }
  updatePlacard();
});
  // Helpers for print & PDF (unchanged)
  function getPlacardHTML() {
    return `
      <div id="placardContent"
           style="border:1px solid #000; padding:2rem; text-align:center;">
        ${quill.root.innerHTML}
      </div>
    `;
  }

  document.getElementById('printPlacard').addEventListener('click', () => {
    const content = getPlacardHTML();
    const w = window.open('', '', 'width=800,height=600');
    w.document.write(`
      <html><head><title>Print Placard</title>
        <style>
          body { display:flex; justify-content:center; align-items:center;
                 height:100vh; margin:0; }
          #placardContent { border:1px solid #000; padding:2rem; text-align:center; }
        </style>
      </head><body>${content}</body></html>`);
    w.document.close();
    w.focus();
    w.print();
  });
  
  function getPlacardHTML() {
  const html = quill.root.innerHTML;
  return `
    <div id="placardContent"
         style="
           width:210mm;
           height:297mm;
           display:flex;
           flex-direction:column;
           justify-content:center;
           align-items:center;
           background:#fff;
         ">
      ${html}
    </div>
  `;
}


// DOWNLOAD PDF – fill entire A4 page
document.getElementById('downloadPlacardPdf').addEventListener('click', async () => {
  const { jsPDF } = window.jspdf;

  // 1) Render your placard into an off-screen DIV
  const tmp = document.createElement('div');
  tmp.innerHTML = getPlacardHTML();
  document.body.appendChild(tmp);
  const canvas = await html2canvas(tmp.querySelector('#placardContent'), { scale: 2 });
  document.body.removeChild(tmp);

  // 2) Convert to PNG data URL
  const imgData = canvas.toDataURL('image/png');

  // 3) Create a new A4 PDF (units in points)
  //    Page size: 595.28 pt × 841.89 pt
  const pdf = new jsPDF({ unit: 'pt', format: 'a4', orientation: 'portrait' });
  const pageWidth  = pdf.internal.pageSize.getWidth();
  const pageHeight = pdf.internal.pageSize.getHeight();

  // 4) Draw the image at 0,0 stretched to fill the page
  pdf.addImage(imgData, 'PNG', 0, 0, pageWidth, pageHeight);

  // 5) Save!
  pdf.save('placard.pdf');
});
</script>